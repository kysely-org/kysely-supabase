{
  "name": "kysely-supabase",
  "version": "0.2.1",
  "description": "Kysely type translators for Supabase",
  "repository": "https://github.com/kysely-org/kysely-supabase.git",
  "homepage": "https://github.com/kysely-org/kysely-supabase",
  "author": "Igal Klebanov <igalklebanov@gmail.com> (https://github.com/igalklebanov)",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "kysely",
    "supabase",
    "postgres",
    "postgresql",
    "typescript",
    "types"
  ],
  "scripts": {
    "biome": "biome",
    "build": "tsup",
    "check:exports": "attw --pack .",
    "check:types": "tsc --noEmit",
    "init:db": "supabase start && supabase db reset && supabase gen types typescript --local > schema.gen.ts",
    "lint": "biome check",
    "prepublishOnly": "biome check && pnpm build && pnpm check:exports",
    "test": "pnpm build && pnpm test:typings",
    "test:typings": "tsd tests/typings"
  },
  "peerDependencies": {
    "kysely": ">= 0.24.0 < 1",
    "supabase": ">= 1.0.0 < 3"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "^2.3.0",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.9.1",
    "kysely": "^0.28.8",
    "supabase": "^2.53.6",
    "tsd": "^0.33.0",
    "tsup": "^8.5.0",
    "type-fest": "^5.1.0",
    "typescript": "^5.9.3"
  },
  "sideEffects": false,
  "packageManager": "pnpm@10.18.1+sha512.77a884a165cbba2d8d1c19e3b4880eee6d2fcabd0d879121e282196b80042351d5eb3ca0935fa599da1dc51265cc68816ad2bddd2a2de5ea9fdf92adbec7cd34"
}
